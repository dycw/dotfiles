unalias p
alias bp import builtins; builtins.print(%1)
alias p import rich; rich.print(%1)
alias pl import rich; rich.print({k: v for k, v in locals().items() if not k.startswith("@")})
alias i import rich; rich.inspect(%1)
alias im import rich; rich.inspect(%1, methods=True)
alias ip import rich; rich.inspect(%1, private=True)
alias id import rich; rich.inspect(%1, dunder=True)
alias ia import rich; rich.inspect(%1, all=True)
